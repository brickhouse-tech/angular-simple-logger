{
  "name": "angular-simple-logger",
  "version": "2.0.0",
  "description": "Basic logger with level logging which can also be independent. AngularJS 1.x.",
  "main": "./dist/angular-simple-logger.js",
  "module": "./dist/angular-simple-logger.esm.js",
  "exports": {
    ".": {
      "import": "./dist/angular-simple-logger.esm.js",
      "require": "./dist/angular-simple-logger.js",
      "default": "./dist/angular-simple-logger.js"
    },
    "./light": {
      "import": "./dist/angular-simple-logger.light.esm.js",
      "require": "./dist/angular-simple-logger.light.js",
      "default": "./dist/angular-simple-logger.light.js"
    }
  },
  "files": [
    "dist",
    "src",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "build": "rollup -c",
    "test": "jest --verbose",
    "test:watch": "jest --watch",
    "lint": "eslint src/",
    "prepublishOnly": "npm run build && npm test"
  },
  "homepage": "https://github.com/brickhouse-tech/angular-simple-logger",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/brickhouse-tech/angular-simple-logger.git"
  },
  "author": "Nicholas McCready",
  "license": "MIT",
  "keywords": [
    "angular",
    "angularjs",
    "logger",
    "logging",
    "debug",
    "console"
  ],
  "peerDependencies": {
    "@brickhouse-tech/angular-lts": "^1.9.0"
  },
  "dependencies": {
    "debug": "^4.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.29.0",
    "@babel/preset-env": "^7.29.0",
    "@brickhouse-tech/angular-lts": "^1.9.0",
    "@eslint/js": "^10.0.1",
    "@rollup/plugin-commonjs": "^28.0.0",
    "@rollup/plugin-node-resolve": "^16.0.0",
    "@rollup/plugin-terser": "^0.4.0",
    "angular-mocks": "^1.8.3",
    "babel-jest": "^30.2.0",
    "eslint": "^10.0.0",
    "globals": "^17.3.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "rollup": "^4.0.0"
  },
  "bugs": {
    "url": "https://github.com/brickhouse-tech/angular-simple-logger/issues"
  }
}
